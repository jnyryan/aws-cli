#!/usr/bin/env node
'use strict';
var doc, docopt, info, opts;

process.title = 'aws';

doc = "Usage:\n  aws regions\n  aws instances [--region=<region>]\n  aws -h | --help | --version\n";

docopt = require('docopt').docopt;

info = require('../package.json');

opts = docopt(doc);

if (opts['--version']) {
  console.log('version', info.version);
}
